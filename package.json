{
  "name": "cypress-framework",
  "version": "1.0.0",
  "description": "E2E Framework Cypress",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless --browser chrome",
    "test:open": "npm run lint && cypress open",
    "lint": "eslint ./cypress/e2e/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "clean:reports": "rm cypress/results/*",
    "combine:reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "generate:reports": "marge cypress/reports/report.json -f report -o cypress/reports",
    "cypress:reports": "npm run cypress:run && npm run combine:reports && npm run generate:reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanDelahozMiranda/cypress_trainning.git"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "cypress"
  ],
  "author": "Juan Pablo De la Hoz Miranda <juan.delahozdevelop.g@gmail.com> (https://github.com/JuanDelahozMiranda)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JuanDelahozMiranda/cypress_trainning/issues"
  },
  "homepage": "https://github.com/JuanDelahozMiranda/cypress_trainning#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cypress": "^12.7.0",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "typescript": "^4.6.4",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-multi-reporters": "^1.6.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
